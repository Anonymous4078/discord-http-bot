{
	"name": "sapphire-bot",
	"version": "1.0.0",
	"packageManager": "yarn@3.2.4",
	"private": true,
	"author": "krish",
	"license": "MIT",
	"main": "dist/Bot.js",
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js"
	},
	"type": "module",
	"scripts": {
		"start": "node dist/Bot.js --enable-source-maps",
		"build": "tsc -b src",
		"watch": "yarn tsc-watch -b src --onSuccess \"yarn start\""
	},
	"dependencies": {
		"@skyra/env-utilities": "^1.1.0",
		"@skyra/http-framework": "^0.15.0",
		"@skyra/logger": "^1.0.1",
		"colorette": "^2.0.19",
		"discord.js": "v13"
	},
	"devDependencies": {
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^18.11.7",
		"discord-api-types": "0.33.5",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": ">=18"
	},
	"prettier": "@sapphire/prettier-config"
}
